{
  "expo": {
    "name": "Text Repeater",
    "slug": "Text Repeater",
    "version": "1.0.0",
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Figtree-Black.ttf",
            "./src/assets/fonts/Figtree-Bold.ttf",
            "./src/assets/fonts/Figtree-ExtraBold.ttf",
            "./src/assets/fonts/Figtree-Light.ttf",
            "./src/assets/fonts/Figtree-Medium.ttf",
            "./src/assets/fonts/Figtree-Regular.ttf",
            "./src/assets/fonts/Figtree-SemiBold.ttf"
          ]
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.textrepeaterapp.main"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.app.textrepeaterapp.main",
      "edgeToEdgeEnabled": true
    }
  }
}
